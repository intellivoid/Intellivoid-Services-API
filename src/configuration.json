{
	"name": "Intellivoid Accounts API",
	"configuration": {
		"logging_enabled": true,
		"root_path": "/intellivoid",
		"debugging_mode": false,
		"framework_signature": true,
		"api_signature": true,
		"khm_enabled": true,
		"firewall_deny": [],
		"headers": {}
	},
	"versions": [
		{
			"version": "v1",
			"enabled": true,
			"methods": [
				{"methods": ["GET", "POST"], "path": "coa/application", "class": "\\Methods\\v1\\CoaGetApplicationMethod"},
				{"methods": ["GET", "POST"], "path": "coa/auth/request_authentication", "class": "\\Methods\\v1\\CoaRequestAuthenticationMethod"},
				{"methods": ["GET", "POST"], "path": "coa/auth/process_authentication", "class": "\\Methods\\v1\\CoaProcessAuthenticationMethod"},
				{"methods": ["GET", "POST"], "path": "coa/auth/get_access_token", "class": "\\Methods\\v1\\CoaGetAccessTokenMethod"},
				{"methods": ["GET", "POST"], "path": "accounts/get_user", "class": "\\Methods\\v1\\AccountsGetUserMethod"},
				{"methods": ["GET", "POST"], "path": "accounts/get_email", "class": "\\Methods\\v1\\AccountsGetEmailMethod"},
				{"methods": ["GET", "POST"], "path": "accounts/get_personal_information", "class": "\\Methods\\v1\\AccountsGetPersonalInformationMethod"},
				{"methods": ["GET", "POST"], "path": "application/settings/get_summary", "class": "\\Methods\\v1\\ApplicationSettingsGetSummaryMethod"},
				{"methods": ["GET", "POST"], "path": "application/settings/dump", "class": "\\Methods\\v1\\ApplicationSettingsDumpMethod"},
				{"methods": ["GET", "POST"], "path": "application/settings/add", "class": "\\Methods\\v1\\ApplicationSettingsAddMethod"},
				{"methods": ["GET", "POST"], "path": "application/settings/clear", "class": "\\Methods\\v1\\ApplicationSettingsClearMethod"},
				{"methods": ["GET", "POST"], "path": "application/settings/get", "class": "\\Methods\\v1\\ApplicationSettingsGetMethod"},
				{"methods": ["GET", "POST"], "path": "application/settings/append", "class": "\\Methods\\v1\\ApplicationSettingsAppendMethod"}
			]
		}
	]
}